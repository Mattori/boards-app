<div class="form-group">
  <label class="col-md-4 control-label">Prénom et nom :</label>
  <div class="col-md-4">
    <div class="input-group">
      <span class="input-group-addon">
        <i class="fa fa-user"></i>
      </span>
      {{input type='disabled' class="form-control" value=developer.identity placeholder='Prénom et nom...'}}
    </div>
  </div>
</div>

<div class="form-group">
  <label class="col-md-4 control-label">Adresse e-mail</label>
  <div class="col-md-4">
    <div class="input-group">
      <span class="input-group-addon">
        <i class="fa fa-envelope"></i>
      </span>
      {{input type='disabled' class="form-control" value=developer.email placeholder='Adresse e-mail...'}}
    </div>
  </div>
</div>
